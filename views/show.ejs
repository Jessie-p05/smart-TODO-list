<!DOCTYPE html>
<html lang="en">
  <head>
    <title>todo list Page</title>

    <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
    <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
    <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

  </head>

  <body>
    <h1>Things to do</h1>
    <section class = "lists-body">
      <ol>
        <li>
          <% for(let userTodoList of userTodoLists) { %>
            <form action="" method="POST" class="">
              <input type ="checkbox" id= "<%=userTodoList.id %>" name ="<%=userTodoList.id %>" class = "checkbox">
              <label for="<%=userTodoList.id %>"> <%=userTodoList.id %></label>
              <button >delete</button>
            </form>
            <form action="" method="POST" class="">
              <label for="<%=userTodoList.type %>"><%=userTodoList.type %></label>
              <input type= "submit" name="userTodoList.type" value="change category">
            </form>
            <% } %>
          </li>
        </ol>
      </section>
      <script type="./routes/users.js"></script>
      <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
      <script type="text/javascript" src="/scripts/app.js"></script>
    </body>
    </html>
